{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
    "react/jsx-filename-extension": [0],
    "import/no-unresolved": [0],
    "import/no-extraneous-dependencies": [0],
    "import/extensions": [0],
    "no-console": [0],
    "jsx-a11y/anchor-is-valid": [0],
    "no-underscore-dangle": [0],
    "class-methods-use-this": [0],
    "react/prop-types": [0]
  },
  "env": {
    "browser": true,
    "node": true
  }
}

